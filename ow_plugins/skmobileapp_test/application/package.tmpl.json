{
    "name": "skmobileapp",
    "author": "Skadate",
    "homepage": "",
    "private": true,
    "config": {
        "ionic_watch": "./config/watch.js",
        "ionic_copy": "./config/copy.js",
        "ionic_webpack": "./config/webpack.config.js",
        "ionic_bundler": "webpack"
    },
    "scripts": {
        "setup": "npm uninstall -g cordova ionic cordova-hot-code-push-cli  && npm install -g cordova@8.1.2  ionic@3.20.0  cordova-hot-code-push-cli@1.1.1",
        "setup_ci": "npm install -g cordova@8.1.2  ionic@3.20.0 cordova-hot-code-push-cli@1.1.1",
        "prepare": "node ./updateManifest.js && npm install && node ./prepare.js && ionic cordova platform add android@6.3.0 && ionic cordova platform add ios@4.5.5 && ionic cordova platform add browser@https://github.com/skalfa/cordova-browser",
        "prepare_ci": "node ./updateManifest.js && npm install && node ./prepare.js && ionic cordova platform add browser@https://github.com/skalfa/cordova-browser",
        "prepare_ios": "node ./updateManifest.js && npm install && node ./prepare.js && ionic cordova platform add ios@4.5.5 && ionic cordova platform add browser@https://github.com/skalfa/cordova-browser",
        "prepare_android": "node ./updateManifest.js && npm install && node ./prepare.js && ionic cordova platform add android@6.3.0 && ionic cordova platform add browser@https://github.com/skalfa/cordova-browser",
        "ionic:build": "node --max-old-space-size=8192 --stack-size=1968 ./node_modules/@ionic/app-scripts/bin/ionic-app-scripts.js build && node ./configure.js",
        "ionic:serve": "ionic-app-scripts serve",
        "ionic:build:after": "node ./configure.js",
        "browser": "ionic-app-scripts serve --sourceMap source-map --iscordovaserve --wwwDir platforms/browser/www/ --buildDir platforms/browser/www/build",
        "test": "karma start ./test-config/karma.conf.js",
        "test-ci": "karma start ./test-config/karma.conf.js --single-run",
        "test-coverage": "karma start ./test-config/karma.conf.js --coverage",
        "e2e": "npm run e2e-update && npm run e2e-test",
        "e2e-test": "protractor ./test-config/protractor.conf.js",
        "e2e-update": "webdriver-manager update --standalone false --gecko false"
    },
